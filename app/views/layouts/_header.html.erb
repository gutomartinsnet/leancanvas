          <%= link_to users_show_path(current_user.username) do %>
            <% if current_user.icon? %>
              <i><%= image_tag current_user.icon ,class: 'user_img_icon' if current_user.icon != nil %></i>
            <% else %>
              <i><%= image_tag current_user.image ,class: 'user_img_icon' if current_user.image != nil %></i>
            <% end %>
          <span><%= current_user.name %></span>
          <% end %>
        </li>
        <% end %>
        <li>
          <%= link_to plans_path do %>
            <i class="fas fa-home"></i>ホーム
          <% end %>
        </li>
