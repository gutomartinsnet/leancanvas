          <%= link_to new_user_session_path do %>
            <i class="fas fa-sign-in-alt"></i>ログイン
          <% end %>
        </li>
        <% end %>
      </ul>
      <% if user_signed_in? %>
        <% if current_user.image.nil? && current_user.icon.nil? %>
          <%= link_to users_show_path(current_user.username), {:class => "user_icon right"} do %>
            <i class="material-icons">account_circle</i>
          <% end %>
        <% else %>
          <%= link_to users_show_path(current_user.username), {:class => "user_icon right"} do %>
            <% if current_user.icon? %>
              <i><%= image_tag current_user.icon ,class: 'user_img_icon' if current_user.icon != nil %></i>
            <% else %>
              <i><%= image_tag current_user.image ,class: 'user_img_icon' if current_user.image != nil %></i>
